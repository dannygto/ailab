    "test:ci": "react-scripts test --coverage --watchAll=false",
    "test:unit": "react-scripts test --watchAll=false --testPathPattern=__tests__",
    "test:integration": "node scripts/integration-test.js",
    "test:e2e": "node scripts/e2e-test.js",
